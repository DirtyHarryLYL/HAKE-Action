  }
}
}
layer {
  name: "part_all_for_lstm"
  type: "Concat"
  bottom: "select_part1_reshape"
  bottom: "select_part2_reshape"
  bottom: "select_part3_reshape"
  bottom: "select_part4_reshape"
  bottom: "select_part5_reshape"
  bottom: "select_part6_reshape" # [1 3 512]
  top: "reshape_lstm_part_all" # [6 3 512]
  concat_param {
    axis: 0
  }
