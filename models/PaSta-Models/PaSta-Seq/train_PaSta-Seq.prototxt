  shape {
    dim: 6
    dim: 1
  }
}
}
layer {
  name: "cont_for_lstm"
  type: "Concat"
  bottom: "pvp_cont_reshape"
  bottom: "pvp_cont_reshape"
  bottom: "pvp_cont_reshape" # [6 1]
  top: "pvp_cont_all" # [6 3]
  concat_param {
    axis: 1
  }
}
layer {
  name: "lstm_for_6pvp"
  type: "LSTM"
  bottom: "reshape_lstm_part_all" # [6 3 512]
  bottom: "pvp_cont_all" # [6 3]
  top: "lstm_6pvp" # [6 3 512]
  recurrent_param {
    num_output: 512
    weight_filler {
      type: "uniform"
      min: -0.08
      max: 0.08
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
### use the last hidden output of lstm to classify 117 and 600
layer {
  name: "lstm_slice"
  type: "Slice"
  bottom: "lstm_6pvp"
  top: "lstm_output_former5" # [5 3 512]
