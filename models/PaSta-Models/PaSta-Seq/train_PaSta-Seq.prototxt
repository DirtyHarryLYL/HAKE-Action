  reshape_param {
  shape {
    dim: 3
    dim: -1
  }
}
}

layer {
  name: "cls_score_117v_"
  type: "InnerProduct"
  bottom: "lstm_output_final1_reshape" # the last output of lstm
  top: "cls_score_117v"
  param {
    lr_mult: 10
    decay_mult: 1
  }
  param {
    lr_mult: 20
    decay_mult: 0
  }
  inner_product_param {
