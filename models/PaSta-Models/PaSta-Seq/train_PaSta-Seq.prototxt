  top: 'object_list'
  top: 'object_list_wts'
  ######################################lstm cont
  top: 'pvp_cont'
  ########################
  python_param {
    module: 'HOI_data_layer-PaSta-Seq'
    layer: 'HOIDataLayer'
    param_str: '{"batch_size":1,"max_size":640, "root":"./data/hico/","list_file":"./data/hico/train_filelist.txt"}'
  }
  include {
    phase: TRAIN
  }
}

